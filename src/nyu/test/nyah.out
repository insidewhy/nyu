include [
    [ "nyu" ]
    [ "nyah" ]
    [ "nyu" ]
]
module id [
    "nyu"
    "grammar"
]
module {
    "meta": NyuGrammar: (
        [ "nyu" ]
        {
            "Identifier": Rule: (
                '-'
                Expression: RuleRef: [
                    "nyu"
                    "Identifier"
                ]
            )
            "ScopedIdentifier": Rule: (
                '-'
                Expression: Join: (
                    RuleRef: [ "Identifier" ]
                    "%+"
                    String: "."
                )
            )
            "ModuleDefinition": Rule: (
                '-'
                Expression: Sequence: [
                    String: "@module"
                    RuleRef: [ "ScopedIdentifier" ]
                ]
            )
            "NyuGrammar": Rule: (
                '='
                Expression: Sequence: [
                    String: "@grammar"
                    Prefix: (
                        '#'
                        RuleRef: [ "Identifier" ]
                    )
                    Suffix: (
                        Expression: Sequence: [
                            String: ":"
                            RuleRef: [ "ScopedIdentifier" ]
                        ]
                        '?'
                    )
                    RuleRef: [
                        "nyu"
                        "Grammar"
                    ]
                ]
            )
            "Module": Rule: (
                '='
                Expression: Sequence: [
                    Prefix: (
                        "#+"
                        Expression: Suffix: (
                            RuleRef: [ "ModuleDefinition" ]
                            '?'
                        )
                    )
                    Suffix: (
                        Expression: OrderedChoice: [
                            RuleRef: [ "NyuGrammar" ]
                            RuleRef: [
                                "nyah"
                                "Class"
                            ]
                        ]
                        '+'
                    )
                ]
            )
            "Include": Rule: (
                '-'
                Expression: Sequence: [
                    String: "@include"
                    Join: (
                        RuleRef: [ "Identifier" ]
                        "%+"
                        String: "/"
                    )
                ]
            )
            "Grammar": Rule: (
                '-'
                Expression: Sequence: [
                    Suffix: (
                        RuleRef: [ "Include" ]
                        '*'
                    )
                    Suffix: (
                        RuleRef: [ "Module" ]
                        '+'
                    )
                ]
            )
        }
    )
    "Rule": Class: {
        "Status": Enum: [
            "UNKNOWN"
            "NORMAL"
            "HASHED"
            "NODE"
        ]
    }
    "nyu": NyuGrammar: (
        empty
        {
            "Spacing": Rule: (
                '-'
                Expression: Suffix: (
                    Expression: OrderedChoice: [
                        "\s"
                        Joined: [
                            String: "//"
                            Suffix: (
                                Expression: Prefix: (
                                    '!'
                                    "\n"
                                )
                                '*'
                            )
                            "\n"
                        ]
                    ]
                    '+'
                )
            )
            "AnyCharacter": Rule: (
                '-'
                Expression: String: "."
            )
            "CharacterRange": Rule: (
                '='
                Expression: Joined: [
                    String: "["
                    Suffix: (
                        Expression: OrderedChoice: [
                            Joined: [
                                CharacterRange: [ "\\" ]
                                CharacterRange: [
                                    "\\"
                                    "\]"
                                    's'
                                    'S'
                                    'n'
                                    'N'
                                    't'
                                    'T'
                                    'w'
                                    'W'
                                ]
                            ]
                            Expression: Joined: [
                                '.'
                                String: "-"
                                '.'
                            ]
                            '.'
                        ]
                        '*'
                    )
                    String: "]"
                ]
            )
            "Escape": Rule: (
                '-'
                Expression: Joined: [
                    CharacterRange: [ "\\" ]
                    CharacterRange: [
                        's'
                        'S'
                        'n'
                        'N'
                        't'
                        'T'
                        'w'
                        'W'
                        '.'
                        '"'
                        '\''
                        '&'
                        '!'
                        '+'
                        '*'
                        "\\"
                    ]
                ]
            )
            "String": Rule: (
                '='
                Expression: OrderedChoice: [
                    Joined: [
                        String: "\""
                        Suffix: (
                            Expression: OrderedChoice: [
                                RuleRef: [ "Escape" ]
                                Prefix: (
                                    '!'
                                    String: "\""
                                )
                            ]
                            "^*"
                        )
                        String: "\""
                    ]
                    Joined: [
                        String: "'"
                        Suffix: (
                            Expression: OrderedChoice: [
                                RuleRef: [ "Escape" ]
                                Prefix: (
                                    '!'
                                    String: "'"
                                )
                            ]
                            "^*"
                        )
                        String: "'"
                    ]
                ]
            )
            "Identifier": Rule: (
                '-'
                Expression: Joined: [
                    CharacterRange: [
                        (
                            'a'
                            'z'
                        )
                        (
                            'A'
                            'Z'
                        )
                        '_'
                    ]
                    Suffix: (
                        CharacterRange: [
                            (
                                'a'
                                'z'
                            )
                            (
                                'A'
                                'Z'
                            )
                            (
                                '0'
                                '9'
                            )
                            '_'
                        ]
                        '+'
                    )
                ]
            )
            "RuleRef": Rule: (
                '='
                Expression: Join: (
                    RuleRef: [ "Identifier" ]
                    "%+"
                    String: "::"
                )
            )
            "Primary": Rule: (
                '-'
                Expression: OrderedChoice: [
                    RuleRef: [ "String" ]
                    RuleRef: [ "CharacterRange" ]
                    RuleRef: [ "Escape" ]
                    RuleRef: [ "AnyCharacter" ]
                    Sequence: [
                        RuleRef: [ "RuleRef" ]
                        Prefix: (
                            "&!"
                            String: "<"
                        )
                    ]
                    Sequence: [
                        String: "("
                        RuleRef: [ "Expression" ]
                        String: ")"
                    ]
                ]
            )
            "Suffix": Rule: (
                '='
                Expression: Sequence: [
                    RuleRef: [ "Primary" ]
                    Suffix: (
                        Expression: OrderedChoice: [
                            String: "^+"
                            String: "^*"
                            CharacterRange: [
                                '?'
                                '*'
                                '+'
                            ]
                            String: "|+"
                            String: "|?"
                        ]
                        "|?"
                    )
                ]
            )
            "Prefix": Rule: (
                '='
                Expression: Sequence: [
                    Suffix: (
                        Expression: OrderedChoice: [
                            String: "&!"
                            String: "#+"
                            CharacterRange: [
                                '&'
                                '!'
                                '#'
                            ]
                        ]
                        "|?"
                    )
                    RuleRef: [ "Suffix" ]
                ]
            )
            "Join": Rule: (
                '='
                Expression: Sequence: [
                    RuleRef: [ "Prefix" ]
                    Expression: OrderedChoice: [
                        String: "^%"
                        String: "%+"
                        String: "%"
                        String: "|%"
                        String: "|^%"
                    ]
                    RuleRef: [ "Prefix" ]
                ]
            )
            "Joined": Rule: (
                '='
                Expression: Join: (
                    Expression: OrderedChoice: [
                        RuleRef: [ "Join" ]
                        RuleRef: [ "Prefix" ]
                    ]
                    "|%"
                    String: "^"
                )
            )
            "Sequence": Rule: (
                '='
                Expression: Suffix: (
                    RuleRef: [ "Joined" ]
                    "|+"
                )
            )
            "OrderedChoice": Rule: (
                '='
                Expression: Join: (
                    RuleRef: [ "Sequence" ]
                    "|%"
                    String: "/"
                )
            )
            "Expression": Rule: (
                '='
                Expression: RuleRef: [ "OrderedChoice" ]
            )
            "Rule": Rule: (
                '='
                Expression: Sequence: [
                    Prefix: (
                        '#'
                        RuleRef: [ "Identifier" ]
                    )
                    Joined: [
                        String: "<"
                        CharacterRange: [
                            '-'
                            '='
                        ]
                    ]
                    RuleRef: [ "Expression" ]
                ]
            )
            "Grammar": Rule: (
                '-'
                Expression: Suffix: (
                    RuleRef: [ "Rule" ]
                    '+'
                )
            )
        }
    )
    "nyah": NyuGrammar: (
        [ "nyu" ]
        {
            "Identifier": Rule: (
                '-'
                Expression: RuleRef: [
                    "nyu"
                    "Identifier"
                ]
            )
            "ScopedIdentifier": Rule: (
                '-'
                Expression: Join: (
                    RuleRef: [ "Identifier" ]
                    "%+"
                    String: "."
                )
            )
            "EnumEntry": Rule: (
                '-'
                Expression: RuleRef: [ "Identifier" ]
            )
            "Enum": Rule: (
                '='
                Expression: Sequence: [
                    String: "enum"
                    Prefix: (
                        '#'
                        RuleRef: [ "Identifier" ]
                    )
                    String: "{"
                    Suffix: (
                        RuleRef: [ "EnumEntry" ]
                        '*'
                    )
                    String: "}"
                ]
            )
            "Attribute": Rule: (
                '='
                Expression: Sequence: [
                    Prefix: (
                        '#'
                        RuleRef: [ "Identifier" ]
                    )
                    String: ":"
                    RuleRef: [ "ScopedIdentifier" ]
                ]
            )
            "Class": Rule: (
                '='
                Expression: Sequence: [
                    String: "class"
                    Prefix: (
                        '#'
                        RuleRef: [ "Identifier" ]
                    )
                    String: "{"
                    Suffix: (
                        Expression: OrderedChoice: [
                            RuleRef: [ "Attribute" ]
                            RuleRef: [ "Enum" ]
                        ]
                        '*'
                    )
                    String: "}"
                ]
            )
        }
    )
}
